{
  "manifest_version": 2,
  "name": "MoveX",
  "version": "0.1",
  "description": "Scroll horizontally with a standard mouse",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": []
}